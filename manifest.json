{
  "manifest_version": 3,
  "name": "Redirect Tracker",
  "version": "1.0",
  "description": "Tracks URL redirects with status codes and headers",
  "permissions": [
    "webRequest",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["popup.html"],
    "matches": ["<all_urls>"]
  }]
} 